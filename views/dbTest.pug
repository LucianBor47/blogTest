html
    head
        title= title
        link(href='/css/style.css', rel='stylesheet')
    body
        h1= message
        br
        h2= 'User name is: ' + name
        h2= 'User email is: ' + email
        h2= 'User id is: ' + id
        ul(class='data')
            h4= "These are the user names in the database: "
            each item in data
                
                li(class='items')= item.id + ". " + item.name + " id: " + item.id